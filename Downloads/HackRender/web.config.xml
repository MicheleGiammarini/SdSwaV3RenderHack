<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Main Rule" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="index.ts" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
